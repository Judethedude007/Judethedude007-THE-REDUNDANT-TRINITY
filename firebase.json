{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/app/**",
        "destination": "/app.html"
      },
      {
        "source": "/app",
        "destination": "/app.html"
      }
    ]
  }
}
